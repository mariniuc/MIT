balance = 33296; annualInterestRate = 0.2; monthlyPaymentRate = 0.6
pay = 0
m = 0
ubalance = round(balance, 2)
for m in range(12):
    pay = monthlyPaymentRate * round(balance, 2)
    ubalance = round(balance, 2) - pay
    balance = round(ubalance, 2) + (annualInterestRate / 12) * round(ubalance, 2)
    m += 1
    print("Remaining balance:", round(balance, 2))
